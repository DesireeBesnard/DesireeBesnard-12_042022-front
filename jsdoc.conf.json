{
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
      "include": ["./src"]
    },
    "plugins": [
      "plugins/markdown", 
      "better-docs/component"
    ],
    "templates": {
      "better-docs": {
        "name": "My React components"
      }
    },
    "opts": {
      "encoding": "utf8",
      "destination": "docs/",
      "recurse": true,
      "verbose": true,
      "template": "./node_modules/better-docs"
    }
  }